(define (chek_date list1)
	(cond ( (and (> dd 32) (> mm 13) (> yy 2016) (< yy 0))
		0)
                ((and (or (= mm 4) (= mm 6) (= mm 9) (= mm 11)) (< dd 31))
                1)
                ((and (or (= mm 1) (= mm 3) (= mm 5) (= mm 7) (= mm 8) (= mm 10) (= mm 12)) (< dd 31))
                1)
                ()))
                
))
