(define (percent_above_avg lst1)
		(cond ((null? lst1) "list is empty")
			(else (* (/ (abv_avg_count marklist) (len marklist)) 100)
 (define (avg lst2)
            (cond ((null? lst2) "list empty")
            		(else (/ (sum marklist) (len marklist)))))
            		
 (define (sum lst3)
            (cond ((null? lst3) 0)
            		(else (+ (car lst3) (sum (cdr lst3))))))
            		
   
